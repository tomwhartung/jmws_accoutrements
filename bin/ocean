#!/bin/bash
#
# ocean: display the OCEAN traits saved in one or more *-score_values.csv files
# -----------------------------------------------------------------------------
#

for FILE in $*; do
   head -1 $FILE
   grep Openness $FILE
   grep Conscientiousness $FILE
   grep Extraversion $FILE
   grep Agreeableness $FILE
   grep Neuroticism $FILE
   echo
done

