#!/bin/bash
#
# msjm: run mysql and access db used by joomoowebsites
# ----------------------------------------------------
#
db_user=joomla
db_name=jumlah3_joomoowebsites

if [ "$1" == '' ]; then
   password_arg="-p"
else
   password_arg="--password=$1"
fi

mysql -u $db_user $password_arg $db_name
